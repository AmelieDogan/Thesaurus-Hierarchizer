    <skos:inScheme rdf:resource="http://data.cmbv.org/vocabulary/"/>
    <skos:prefLabel xml:lang="fr">Basse chiffrée</skos:prefLabel>
    <skos:definition xml:lang="fr">Codification en chiffres des accords sur une voix de basse, pour la réalisation de la basse continue (XVI-XIXe siècles).</skos:definition>
    <skos:scopeNote xml:lang="fr"/>
    <skos:changeNote xml:lang="fr">EP345656</skos:changeNote>
  </skos:Concept>
  <skos:Concept rdf:about="http://data.cmbv.org/vocabulary/02d61acf-2568-42c9-b825-7ad4d7e7ea14">
    <skos:inScheme rdf:resource="http://data.cmbv.org/vocabulary/"/>
    <skos:prefLabel xml:lang="fr">Basse continue</skos:prefLabel>
    <skos:definition xml:lang="fr">Système d'accompagnement musical en usage du XVIe siècle au début du XIXe siècle. Un groupe d'instruments mélodiques (violoncelle, contrebasse, basson, cromorne, etc.) et un groupe d'instruments harmoniques (orgue, clavecin, théorbe, luth, etc.) réalisent la ligne de basse d'une pièce tout en l'agrémentant de l'harmonie par des accords improvisés.</skos:definition>
    <skos:broader rdf:resource="http://data.cmbv.org/vocabulary/0a328b33-4e1f-4ef4-8853-e32a6f723852"/>
