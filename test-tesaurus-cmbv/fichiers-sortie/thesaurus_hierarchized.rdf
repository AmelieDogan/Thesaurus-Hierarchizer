    <skos:inScheme rdf:resource="http://data.cmbv.org/vocabulary/"/>
    <skos:prefLabel xml:lang="fr">Clarinette</skos:prefLabel>
    <skos:definition xml:lang="fr">Instrument à anche simple, la clarinette possède une tessiture très étendue, une grande possibilité de nuances, et existe en plusieurs tailles.</skos:definition>
    <skos:narrower rdf:resource="http://data.cmbv.org/vocabulary/bf2a1607-2682-4c5e-9704-09b70ad4c23a"/>
