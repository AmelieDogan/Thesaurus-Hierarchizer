  <skos:Concept rdf:about="http://data.cmbv.org/vocabulary/deec2151-2df8-438f-9550-3fa01148b4e6">
    <skos:inScheme rdf:resource="http://data.cmbv.org/vocabulary/"/>
    <skos:prefLabel xml:lang="fr">Accord</skos:prefLabel>
    <skos:definition xml:lang="fr">Désigne l'émission simultanée de sons.</skos:definition>
    <skos:broader rdf:resource="http://data.cmbv.org/vocabulary/10d8a95e-1fea-401e-ad73-207461b30619"/>
