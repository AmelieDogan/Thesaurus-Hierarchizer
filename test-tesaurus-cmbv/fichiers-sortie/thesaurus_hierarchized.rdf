  <skos:Concept rdf:about="http://data.cmbv.org/vocabulary/83e770d3-c674-4807-b15d-c716b6d13226">
    <skos:inScheme rdf:resource="http://data.cmbv.org/vocabulary/"/>
    <skos:prefLabel xml:lang="fr">Bibliographie</skos:prefLabel>
    <skos:definition xml:lang="fr">Liste des écrits relatifs à un sujet donné, à un auteur ou servant de référence. (Le Robert)</skos:definition>
    <skos:scopeNote xml:lang="fr"/>
    <skos:changeNote xml:lang="fr">PMB22880</skos:changeNote>
  </skos:Concept>
  <skos:Concept rdf:about="http://data.cmbv.org/vocabulary/c4b2ef8e-0bc5-44c9-ba3b-f8904203907f">
    <skos:inScheme rdf:resource="http://data.cmbv.org/vocabulary/"/>
    <skos:prefLabel xml:lang="fr">Bibliothécaire</skos:prefLabel>
    <skos:definition xml:lang="fr">Personne chargée de la gestion d'une bibliothèque (acquisition de livres, classement, conservation, communication).</skos:definition>
