  <skos:Concept rdf:about="http://data.cmbv.org/vocabulary/e53dc3b9-fbd0-4399-bb23-454957dd6ef7">
    <skos:inScheme rdf:resource="http://data.cmbv.org/vocabulary/"/>
    <skos:prefLabel xml:lang="fr">Baptême</skos:prefLabel>
    <skos:definition xml:lang="fr">Rite chrétien marquant l’entrée d’une personne dans la communauté ecclésiale, symbolisé par une immersion ou une aspersion d’eau. Il est traditionnellement associé à une démarche de renouveau spirituel.</skos:definition>
    <skos:altLabel xml:lang="fr">Rite d’initiation chrétienne</skos:altLabel>
    <skos:altLabel xml:lang="fr">Sacrement du baptême</skos:altLabel>
    <skos:broader rdf:resource="http://data.cmbv.org/vocabulary/6b077aa3-48ed-4fa0-b4e4-2d3876121d91"/>
    <skos:narrower rdf:resource="http://data.cmbv.org/vocabulary/a5b32839-12bb-4c22-b59e-d02e89f9bb10"/>
