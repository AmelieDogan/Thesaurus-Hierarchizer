  <skos:Concept rdf:about="http://data.cmbv.org/vocabulary/8077de58-e19a-4ad6-814f-78aade79adc1">
    <skos:inScheme rdf:resource="http://data.cmbv.org/vocabulary/"/>
    <skos:prefLabel xml:lang="fr">Almanach musical</skos:prefLabel>
    <skos:definition xml:lang="fr">Publication annuelle spécialisée dans le domaine musical, contenant des articles, calendriers de concerts, notices biographiques, ou répertoires d’institutions musicales.</skos:definition>
