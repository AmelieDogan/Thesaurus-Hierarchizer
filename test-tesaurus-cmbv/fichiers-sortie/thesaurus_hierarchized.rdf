    <skos:inScheme rdf:resource="http://data.cmbv.org/vocabulary/"/>
    <skos:prefLabel xml:lang="fr">Clarinettiste</skos:prefLabel>
    <skos:definition xml:lang="fr">Musicien qui joue de la clarinette.</skos:definition>
    <skos:scopeNote xml:lang="fr"/>
