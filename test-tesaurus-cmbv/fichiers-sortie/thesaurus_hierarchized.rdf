  <skos:Concept rdf:about="http://data.cmbv.org/vocabulary/5b6655b1-b766-48ee-b7b1-b8b13728f818">
    <skos:inScheme rdf:resource="http://data.cmbv.org/vocabulary/"/>
    <skos:prefLabel xml:lang="fr">Anthem</skos:prefLabel>
    <skos:definition xml:lang="fr">Forme vocale de musique lithurgique anglicane, s'appuyant sur des textes en langue vulgaire traduits de la Bible, qui se met en place au cours du règne Elisabeth I, sous l'influence de Thomas Tallis, de Gibbons et surtout de Byrd. (Bouissou, Sylvie (1996). Vocabulaire de la musique baroque. Minerve.)</skos:definition>
    <skos:scopeNote xml:lang="fr"/>
    <skos:changeNote xml:lang="fr">PMB1138</skos:changeNote>
  </skos:Concept>
