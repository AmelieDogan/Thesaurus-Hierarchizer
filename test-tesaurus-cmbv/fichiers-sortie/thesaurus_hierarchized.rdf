    <skos:scopeNote xml:lang="fr"/>
    <skos:changeNote xml:lang="fr">EP346097;PMB1143</skos:changeNote>
  </skos:Concept>
  <skos:Concept rdf:about="http://data.cmbv.org/vocabulary/7a33d886-b4d0-4519-b618-83773b761b2f">
    <skos:inScheme rdf:resource="http://data.cmbv.org/vocabulary/"/>
    <skos:prefLabel xml:lang="fr">Archetier</skos:prefLabel>
    <skos:definition xml:lang="fr">Artisan qui fabrique des archets.</skos:definition>
    <skos:scopeNote xml:lang="fr"/>
    <skos:changeNote xml:lang="fr">EP346089</skos:changeNote>
  </skos:Concept>
  <skos:Concept rdf:about="http://data.cmbv.org/vocabulary/eac8108c-c075-4c19-b8e7-e7835bc8f0fb">
    <skos:inScheme rdf:resource="http://data.cmbv.org/vocabulary/"/>
    <skos:prefLabel xml:lang="fr">Archichantre</skos:prefLabel>
    <skos:definition xml:lang="fr">Dignité ecclésiastique, directeur des chantres. (Escudier, Léon et Marie (1872). Dictionnaire de musique théorique et historique (5e éd.). E. Dentu.)</skos:definition>
