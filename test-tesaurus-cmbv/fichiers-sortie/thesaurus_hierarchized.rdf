    <skos:inScheme rdf:resource="http://data.cmbv.org/vocabulary/"/>
    <skos:prefLabel xml:lang="fr">Antienne de communion</skos:prefLabel>
    <skos:definition xml:lang="fr">Chant antiphonaire pour la partie de la messe où les fidèles reçoivent le pain et le vin consacrés.</skos:definition>
    <skos:broader rdf:resource="http://data.cmbv.org/vocabulary/6237a472-f79c-40de-b33f-1a12f7f31ea2"/>
