    <skos:scopeNote xml:lang="fr"/>
    <skos:changeNote xml:lang="fr">EP346382</skos:changeNote>
  </skos:Concept>
  <skos:Concept rdf:about="http://data.cmbv.org/vocabulary/a2ed65d8-6eb0-40a3-bb73-0ef9b2f8cde4">
    <skos:inScheme rdf:resource="http://data.cmbv.org/vocabulary/"/>
    <skos:prefLabel xml:lang="fr">Maîtrise</skos:prefLabel>
    <skos:definition xml:lang="fr">Lieu d'éducation musicale des chantres, souvent lié à une institution religieuse, qui forme les chanteurs et instrumentistes de la période baroque jusqu'à la Révolution. Il est ensuite remplacé en partie par les conservatoires.</skos:definition>
    <skos:broader rdf:resource="http://data.cmbv.org/vocabulary/3f89149a-cc83-4600-82cc-7297f526938c"/>
