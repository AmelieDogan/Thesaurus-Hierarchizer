  <skos:Concept rdf:about="http://data.cmbv.org/vocabulary/b9d30e53-86fe-4667-923b-8b405b1dac7c">
    <skos:inScheme rdf:resource="http://data.cmbv.org/vocabulary/"/>
    <skos:prefLabel xml:lang="fr">Ballet héroïque</skos:prefLabel>
    <skos:definition xml:lang="fr">Cousin de l'opéra-ballet, il s'en distingue par la présence de figures héroïques tirées du monde antique et par son caractère joyeux, contrairement à la tragédie en musique.</skos:definition>
    <skos:broader rdf:resource="http://data.cmbv.org/vocabulary//3fb71cc9-d967-40f0-86d9-89a98b5fcc8c"/>
