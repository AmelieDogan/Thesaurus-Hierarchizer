    <skos:prefLabel xml:lang="fr">Ariette instrumentale</skos:prefLabel>
    <skos:definition xml:lang="fr"/>
    <skos:broader rdf:resource="http://data.cmbv.org/vocabulary/0ac7e49b-19f9-45a9-b47f-edbd995f782d"/>
    <skos:narrower rdf:resource="http://data.cmbv.org/vocabulary/48564189-2389-4114-bfaa-8190e9df973b"/>
