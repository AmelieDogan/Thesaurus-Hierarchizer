    <skos:scopeNote xml:lang="fr"/>
    <skos:changeNote xml:lang="fr"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://data.cmbv.org/vocabulary/89b70f99-2529-4aae-94b2-69a0257ff9c8">
    <skos:inScheme rdf:resource="http://data.cmbv.org/vocabulary/"/>
    <skos:prefLabel xml:lang="fr">Carême, dimanche II</skos:prefLabel>
    <skos:definition xml:lang="fr">Deuxième dimanche du Carême, traditionnellement associé à la Transfiguration du Christ dans les lectures bibliques.</skos:definition>
    <skos:altLabel xml:lang="fr">2e dimanche de Carême</skos:altLabel>
    <skos:scopeNote xml:lang="fr"/>
    <skos:changeNote xml:lang="fr"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://data.cmbv.org/vocabulary/b9ffdc16-d383-46c0-851f-b1099c6cf815">
    <skos:inScheme rdf:resource="http://data.cmbv.org/vocabulary/"/>
    <skos:prefLabel xml:lang="fr">Carême, dimanche III</skos:prefLabel>
    <skos:definition xml:lang="fr">Troisième dimanche de Carême, axé dans la liturgie sur le thème de la conversion et de la miséricorde divine.</skos:definition>
    <skos:altLabel xml:lang="fr">3e dimanche de Carême</skos:altLabel>
    <skos:scopeNote xml:lang="fr"/>
    <skos:changeNote xml:lang="fr"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://data.cmbv.org/vocabulary/7738de00-7fc5-482d-9db6-7030ebc20b9c">
    <skos:inScheme rdf:resource="http://data.cmbv.org/vocabulary/"/>
    <skos:prefLabel xml:lang="fr">Carême, dimanche IV</skos:prefLabel>
    <skos:definition xml:lang="fr">Quatrième dimanche du Carême, appelé aussi dimanche Laetare, marquant une pause joyeuse dans l’austérité du Carême.</skos:definition>
    <skos:altLabel xml:lang="fr">4e dimanche de Carême</skos:altLabel>
    <skos:altLabel xml:lang="fr">Laetare</skos:altLabel>
    <skos:scopeNote xml:lang="fr"/>
    <skos:changeNote xml:lang="fr"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://data.cmbv.org/vocabulary/84c6ed63-0661-4c21-bcb0-3d17805877c9">
    <skos:inScheme rdf:resource="http://data.cmbv.org/vocabulary/"/>
    <skos:prefLabel xml:lang="fr">Carême, dimanche V</skos:prefLabel>
    <skos:definition xml:lang="fr">Cinquième dimanche de Carême, précédant immédiatement la Semaine sainte, souvent centré sur la perspective de la Passion du Christ.</skos:definition>
    <skos:altLabel xml:lang="fr">5e dimanche de Carême</skos:altLabel>
    <skos:scopeNote xml:lang="fr"/>
    <skos:changeNote xml:lang="fr"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://data.cmbv.org/vocabulary/3b515062-abb2-4ce9-81ba-a0d4566576b6">
    <skos:inScheme rdf:resource="http://data.cmbv.org/vocabulary/"/>
    <skos:prefLabel xml:lang="fr">Carillon</skos:prefLabel>
    <skos:definition xml:lang="fr">Réunion de cloches accordées de manière à former une échelle chromatique. Le premier carillon fut exécuté à Alost, en Flandre, en 1487. (Escudier, Léon et Marie (1872). Dictionnaire de musique théorique et historique (5e éd.). E. Dentu.)</skos:definition>
    <skos:scopeNote xml:lang="fr"/>
    <skos:changeNote xml:lang="fr"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://data.cmbv.org/vocabulary/4fdd5339-220e-4a2b-974a-b445bf2b712c">
    <skos:inScheme rdf:resource="http://data.cmbv.org/vocabulary/"/>
    <skos:prefLabel xml:lang="fr">Carillonneur</skos:prefLabel>
    <skos:definition xml:lang="fr">Musicien qui joue du carillon.</skos:definition>
    <skos:scopeNote xml:lang="fr"/>
