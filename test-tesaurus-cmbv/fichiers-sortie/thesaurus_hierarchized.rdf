    <skos:scopeNote xml:lang="fr"/>
    <skos:changeNote xml:lang="fr">EP345662</skos:changeNote>
  </skos:Concept>
  <skos:Concept rdf:about="http://data.cmbv.org/vocabulary/4c058c9f-e6f7-4681-82a9-61b771f94eee">
    <skos:inScheme rdf:resource="http://data.cmbv.org/vocabulary/"/>
    <skos:prefLabel xml:lang="fr">Accord de dominante</skos:prefLabel>
    <skos:definition xml:lang="fr">Il est composé de la dominante, de sa tierce, de sa quinte et de sa septième. La dissonance et les attractions multipliées qu'il renferme lui donnent l'expression du mouvement. (Voyez le mot HARMONIE.) (Escudier, Léon et Marie (1872). Dictionnaire de musique théorique et historique (5e éd.). E. Dentu.)</skos:definition>
    <skos:broader rdf:resource="http://data.cmbv.org/vocabulary/231f056b-e09c-455c-9554-cfae0918fc9e"/>
    <skos:scopeNote xml:lang="fr"/>
    <skos:changeNote xml:lang="fr"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://data.cmbv.org/vocabulary/231f056b-e09c-455c-9554-cfae0918fc9e">
    <skos:inScheme rdf:resource="http://data.cmbv.org/vocabulary/"/>
    <skos:prefLabel xml:lang="fr">Accord parfait</skos:prefLabel>
    <skos:definition xml:lang="fr">Il est composé de la tonique, de la tierce majeure ou mineure et de sa quinte. C'est le plus doux à l'oreille, et le seul qui donne le sentiment d'une conclusion harmonique. (Escudier, Léon et Marie (1872). Dictionnaire de musique théorique et historique (5e éd.). E. Dentu.)</skos:definition>
    <skos:broader rdf:resource="http://data.cmbv.org/vocabulary/deec2151-2df8-438f-9550-3fa01148b4e6"/>
    <skos:broader rdf:resource="http://data.cmbv.org/vocabulary/deec2151-2df8-438f-9550-3fa01148b4e6"/>
