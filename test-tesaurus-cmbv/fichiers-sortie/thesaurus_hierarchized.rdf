  <skos:Concept rdf:about="http://data.cmbv.org/vocabulary/785deca8-37aa-4765-91e2-b0a8d896fb11">
    <skos:inScheme rdf:resource="http://data.cmbv.org/vocabulary/"/>
    <skos:prefLabel xml:lang="fr">Basilique</skos:prefLabel>
    <skos:definition xml:lang="fr">Église à laquelle le pape a conféré un titre honorifique en raison de son importance. Ce titre est souvent attribué à des lieux de pèlerinage.</skos:definition>
    <skos:scopeNote xml:lang="fr"/>
    <skos:changeNote xml:lang="fr"/>
