    <skos:inScheme rdf:resource="http://data.cmbv.org/vocabulary/"/>
    <skos:prefLabel xml:lang="fr">Antienne d'offertoire</skos:prefLabel>
    <skos:definition xml:lang="fr">Chant antiphonaire pour la partie de la messe correspondant à la bénédiction du pain et du vin.</skos:definition>
