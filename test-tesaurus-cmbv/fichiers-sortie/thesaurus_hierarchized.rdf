  <skos:Concept rdf:about="http://data.cmbv.org/vocabulary/7abde52b-498f-435d-a8a6-5c8e0a7c834a">
    <skos:inScheme rdf:resource="http://data.cmbv.org/vocabulary/"/>
    <skos:prefLabel xml:lang="fr">Beaux-arts</skos:prefLabel>
    <skos:definition xml:lang="fr">Ensemble des disciplines artistiques consacrées à la création d’œuvres, appréciées pour leur valeur esthétique ou expressive. Traditionnellement, les Beaux-Arts comprennent l’architecture, la sculpture, la peinture, la gravure, la musique et, plus tardivement, la poésie et la danse.</skos:definition>
    <skos:broader rdf:resource="http://data.cmbv.org/vocabulary/d5c98bec-0e64-476d-ba25-5cb175359041"/>
    <skos:narrower rdf:resource="http://data.cmbv.org/vocabulary/c9f4e4b6-cc41-4182-8133-f612466e75a5"/>
    <skos:narrower rdf:resource="http://data.cmbv.org/vocabulary/fceca460-da61-42c0-8a9c-267ef603a40c"/>
    <skos:narrower rdf:resource="http://data.cmbv.org/vocabulary/a6fbdd86-2d9f-46b1-a580-82faab62809b"/>
    <skos:narrower rdf:resource="http://data.cmbv.org/vocabulary/8cfa0435-dc1d-4b2c-b8d9-3e8c923286f9"/>
    <skos:scopeNote xml:lang="fr"/>
