    <skos:definition xml:lang="fr"/>
    <skos:scopeNote xml:lang="fr"/>
    <skos:changeNote xml:lang="fr"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://data.cmbv.org/vocabulary/ab21cdf0-6236-45c4-8ab5-561ffd525eea">
    <skos:inScheme rdf:resource="http://data.cmbv.org/vocabulary/"/>
    <skos:prefLabel xml:lang="fr">Berceuse</skos:prefLabel>
    <skos:definition xml:lang="fr">Nom donné à un genre de chanson faite pour endormir les enfants. (Escudier, Léon et Marie (1872). Dictionnaire de musique théorique et historique (5e éd.). E. Dentu.)</skos:definition>
    <skos:scopeNote xml:lang="fr"/>
