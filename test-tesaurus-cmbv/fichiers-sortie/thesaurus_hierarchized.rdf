    <skos:inScheme rdf:resource="http://data.cmbv.org/vocabulary/"/>
    <skos:prefLabel xml:lang="fr">Affiche</skos:prefLabel>
    <skos:definition xml:lang="fr">Feuille imprimée et placardée afin de porter quelque chose à la connaissance du public.</skos:definition>
    <skos:scopeNote xml:lang="fr"/>
