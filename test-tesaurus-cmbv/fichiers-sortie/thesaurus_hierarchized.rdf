    <skos:inScheme rdf:resource="http://data.cmbv.org/vocabulary/"/>
    <skos:prefLabel xml:lang="fr">Carême</skos:prefLabel>
    <skos:definition xml:lang="fr">Période de quarante jours précédant Pâques dans le calendrier liturgique chrétien, consacrée à la pénitence, au jeûne et à la préparation spirituelle. Elle commence avec le mercredi des Cendres et se termine à la veillée pascale.</skos:definition>
    <skos:altLabel xml:lang="fr">Quadragésime</skos:altLabel>
    <skos:narrower rdf:resource="http://data.cmbv.org/vocabulary/455a3aa8-72be-42b5-a20b-bd5849633ccb"/>
    <skos:scopeNote xml:lang="fr"/>
    <skos:changeNote xml:lang="fr">PMB1206</skos:changeNote>
  </skos:Concept>
  <skos:Concept rdf:about="http://data.cmbv.org/vocabulary/455a3aa8-72be-42b5-a20b-bd5849633ccb">
    <skos:inScheme rdf:resource="http://data.cmbv.org/vocabulary/"/>
    <skos:prefLabel xml:lang="fr">Carême, dimanche I</skos:prefLabel>
    <skos:definition xml:lang="fr">Premier dimanche de la période de Carême, marqué dans la liturgie chrétienne par l’évocation du jeûne de Jésus au désert.</skos:definition>
    <skos:altLabel xml:lang="fr">1er dimanche de Carême</skos:altLabel>
    <skos:broader rdf:resource="http://data.cmbv.org/vocabulary/f89fe8c2-85ee-44c9-b393-3f34d884aab3"/>
