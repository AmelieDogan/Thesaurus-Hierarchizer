  <skos:Concept rdf:about="http://data.cmbv.org/vocabulary/da29e6c0-1f7c-45c0-aa0d-da4e70f4d69c">
    <skos:inScheme rdf:resource="http://data.cmbv.org/vocabulary/"/>
    <skos:prefLabel xml:lang="fr">Communauté religieuse</skos:prefLabel>
    <skos:definition xml:lang="fr">Groupe de personnes consacrées vivant ensemble selon une règle commune, généralement dans un lieu dédié, et partageant un mode de vie structuré par la prière, les activités communautaires et un engagement religieux spécifique.</skos:definition>
    <skos:altLabel xml:lang="fr">Communauté monastique</skos:altLabel>
    <skos:altLabel xml:lang="fr">Ordre religieux</skos:altLabel>
    <skos:altLabel xml:lang="fr">Congrégation religieuse</skos:altLabel>
    <skos:altLabel xml:lang="fr">Vie communautaire religieuse</skos:altLabel>
    <skos:narrower rdf:resource="http://data.cmbv.org/vocabulary/4c6be39d-9fc1-4d9b-ba0f-038e6feda769"/>
    <skos:scopeNote xml:lang="fr"/>
    <skos:changeNote xml:lang="fr">EP347040</skos:changeNote>
  </skos:Concept>
  <skos:Concept rdf:about="http://data.cmbv.org/vocabulary/659e10a3-1c8b-4f89-a1f9-24a429a95846">
    <skos:inScheme rdf:resource="http://data.cmbv.org/vocabulary/"/>
    <skos:prefLabel xml:lang="fr">Communion</skos:prefLabel>
    <skos:definition xml:lang="fr">Reception du Corps et du Sang du Christ par les fidèles assistant à la messe sous la forme de pain et de vin.</skos:definition>
    <skos:broader rdf:resource="http://data.cmbv.org/vocabulary/7e69a91c-123f-4310-ae5e-64b25eeaf45d"/>
    <skos:narrower rdf:resource="http://data.cmbv.org/vocabulary/31bea78a-cb31-47fa-bce5-038f790e7ae7"/>
    <skos:scopeNote xml:lang="fr"/>
    <skos:changeNote xml:lang="fr"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://data.cmbv.org/vocabulary/9f576060-da55-4710-86ef-a8745964e96f">
    <skos:inScheme rdf:resource="http://data.cmbv.org/vocabulary/"/>
    <skos:prefLabel xml:lang="fr">Compagnie de jésus</skos:prefLabel>
    <skos:definition xml:lang="fr">Ordre religieux catholique fondé en 1540 par Ignace de Loyola, également connu sous le nom de Jésuites. La Compagnie de Jésus se caractérise par une forte implication dans l’éducation, les missions, les sciences, la spiritualité ignatienne et le dialogue interculturel.</skos:definition>
    <skos:altLabel xml:lang="fr">Jésuites</skos:altLabel>
    <skos:scopeNote xml:lang="fr"/>
    <skos:changeNote xml:lang="fr">PMB1257;PMB1663</skos:changeNote>
  </skos:Concept>
  <skos:Concept rdf:about="http://data.cmbv.org/vocabulary/24a967ad-c759-4088-824d-d2ef3c994af9">
    <skos:inScheme rdf:resource="http://data.cmbv.org/vocabulary/"/>
    <skos:prefLabel xml:lang="fr">Compas</skos:prefLabel>
    <skos:definition xml:lang="fr">Instrument composé de deux branches mobiles que l'on écarte plus ou moins pour mesurer des angles, tracer des circonférences. (Le Robert)</skos:definition>
    <skos:scopeNote xml:lang="fr"/>
    <skos:changeNote xml:lang="fr"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://data.cmbv.org/vocabulary/b8c9a82f-56ed-4d59-a279-ed229bc755e4">
    <skos:inScheme rdf:resource="http://data.cmbv.org/vocabulary/"/>
    <skos:prefLabel xml:lang="fr">Compassion de Marie</skos:prefLabel>
    <skos:definition xml:lang="fr">Dévotion chrétienne évoquant la participation spirituelle de Marie aux souffrances du Christ, en particulier lors de sa Passion et de sa mort.</skos:definition>
    <skos:altLabel xml:lang="fr">Notre-Dame des Sept Douleurs</skos:altLabel>
    <skos:altLabel xml:lang="fr">Fête de la Compassion</skos:altLabel>
    <skos:altLabel xml:lang="fr">Sept Douleurs de la Vierge Marie</skos:altLabel>
    <skos:altLabel xml:lang="fr">15 septembre</skos:altLabel>
    <skos:altLabel xml:lang="fr">Sept douleurs</skos:altLabel>
    <skos:altLabel xml:lang="fr">Mater Dolorosa</skos:altLabel>
    <skos:scopeNote xml:lang="fr">Cette notion est liée à la représentation de Marie comme « Notre-Dame des Douleurs ». Elle est liturgiquement commémorée le 15 septembre, au lendemain de la fête de la Croix glorieuse, dans le calendrier romain. La compassion de Marie est souvent symbolisée par l'image des « sept douleurs » (prophétie de Siméon, fuite en Égypte, perte de Jésus au temple, rencontre sur le chemin de croix, crucifixion, descente de croix, mise au tombeau).</skos:scopeNote>
    <skos:changeNote xml:lang="fr"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://data.cmbv.org/vocabulary/bedc2efa-af9d-4ec2-9b70-0ebcbdf1a001">
    <skos:inScheme rdf:resource="http://data.cmbv.org/vocabulary/"/>
    <skos:prefLabel xml:lang="fr">Complainte</skos:prefLabel>
    <skos:definition xml:lang="fr">Espèce de romance populaire, d'un genre pathétique. Ce petit poème est ordinairement le récit d'une histoire lamentable, qu'on suppose fait par le personnage même. (Escudier, Léon et Marie (1872). Dictionnaire de musique théorique et historique (5e éd.). E. Dentu.)</skos:definition>
    <skos:broader rdf:resource="http://data.cmbv.org/vocabulary/0e906e4e-f8cb-42bd-9ea6-a5fd8cfb360e"/>
    <skos:narrower rdf:resource="http://data.cmbv.org/vocabulary/73c50224-5da6-4761-a8f9-7de80b6f54aa"/>
    <skos:scopeNote xml:lang="fr"/>
    <skos:changeNote xml:lang="fr">EP346750;PMB23380</skos:changeNote>
  </skos:Concept>
  <skos:Concept rdf:about="http://data.cmbv.org/vocabulary/64802d17-9fee-49cd-abc8-ca40fab48e6f">
    <skos:inScheme rdf:resource="http://data.cmbv.org/vocabulary/"/>
    <skos:prefLabel xml:lang="fr">Complies</skos:prefLabel>
    <skos:definition xml:lang="fr">Dernier office de la journée dans la Liturgie des Heures, célébré avant le repos nocturne.</skos:definition>
    <skos:altLabel xml:lang="fr">Office de nuit</skos:altLabel>
    <skos:scopeNote xml:lang="fr">Comprend souvent l’hymne Salve Regina ou d’autres prières mariales.</skos:scopeNote>
    <skos:changeNote xml:lang="fr">EP345884;PMB1258</skos:changeNote>
  </skos:Concept>
  <skos:Concept rdf:about="http://data.cmbv.org/vocabulary/70b04a4b-72ec-4150-8284-4369a72dccbe">
    <skos:inScheme rdf:resource="http://data.cmbv.org/vocabulary/"/>
    <skos:prefLabel xml:lang="fr">Compositeur</skos:prefLabel>
    <skos:definition xml:lang="fr">Celui qui compose de la musique d'après les règles de l'art. (Escudier, Léon et Marie (1872). Dictionnaire de musique théorique et historique (5e éd.). E. Dentu.)</skos:definition>
