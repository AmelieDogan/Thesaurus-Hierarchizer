    <skos:inScheme rdf:resource="http://data.cmbv.org/vocabulary/"/>
    <skos:prefLabel xml:lang="fr">Almanach royal</skos:prefLabel>
    <skos:definition xml:lang="fr">Publication officielle annuelle de l’Ancien Régime listant les institutions royales, les membres de la cour, les charges de l’État et les maisons royales, ainsi que les principales personnalités du royaume.</skos:definition>
    <skos:broader rdf:resource="http://data.cmbv.org/vocabulary/3def7018-9b55-4165-8856-0a4e4ee3d552"/>
