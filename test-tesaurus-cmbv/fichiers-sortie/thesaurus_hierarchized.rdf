    <skos:inScheme rdf:resource="http://data.cmbv.org/vocabulary/"/>
    <skos:prefLabel xml:lang="fr">Autobiographie</skos:prefLabel>
    <skos:definition xml:lang="fr">Biographie d'un auteur faite par lui-même. (Le Robert)</skos:definition>
    <skos:scopeNote xml:lang="fr"/>
    <skos:changeNote xml:lang="fr">EP345786</skos:changeNote>
  </skos:Concept>
  <skos:Concept rdf:about="http://data.cmbv.org/vocabulary/e02c34cc-a19f-4574-8f94-9f438a5aa0cf">
    <skos:inScheme rdf:resource="http://data.cmbv.org/vocabulary/"/>
    <skos:prefLabel xml:lang="fr">Autographe</skos:prefLabel>
    <skos:definition xml:lang="fr">Manuscrit écrit de la main même de son auteur. Dans le domaine musical, il s'agit d'une partition ou d’un document rédigé directement par le compositeur.</skos:definition>
    <skos:narrower rdf:resource="http://data.cmbv.org/vocabulary/640f5695-5f74-4fe1-8218-c4d9c69cab00"/>
    <skos:scopeNote xml:lang="fr"/>
    <skos:changeNote xml:lang="fr">PMB23358</skos:changeNote>
  </skos:Concept>
  <skos:Concept rdf:about="http://data.cmbv.org/vocabulary/acc53009-a7eb-4158-853b-10eb4815c4b3">
    <skos:inScheme rdf:resource="http://data.cmbv.org/vocabulary/"/>
    <skos:prefLabel xml:lang="fr">Automate</skos:prefLabel>
    <skos:definition xml:lang="fr">Machine, figure animée par certains mécanismes capables d'actions simulant des gestes humains ou d'animaux (musiciens jouant, oiseau chantant, chien aboyant...). Certains comportent une mécanique rapportée, propre à émettre les sons appropriés. (Benoit, Marcelle (dir.) (1992). Dictionnaire de la musique en France aux XVIIe et XVIIe siècles. Fayard.)</skos:definition>
    <skos:scopeNote xml:lang="fr"/>
    <skos:changeNote xml:lang="fr">PMB23354</skos:changeNote>
  </skos:Concept>
  <skos:Concept rdf:about="http://data.cmbv.org/vocabulary/893cf694-2b8d-415c-9cbc-da3fa9ececf7">
    <skos:inScheme rdf:resource="http://data.cmbv.org/vocabulary/"/>
    <skos:prefLabel xml:lang="fr">Automne musical</skos:prefLabel>
    <skos:definition xml:lang="fr"/>
    <skos:narrower rdf:resource="http://data.cmbv.org/vocabulary/b3335312-2929-4772-aaf4-cb1267f23794"/>
    <skos:narrower rdf:resource="http://data.cmbv.org/vocabulary/704eb7db-0346-4859-8858-6f779fd3e66f"/>
    <skos:scopeNote xml:lang="fr"/>
    <skos:changeNote xml:lang="fr">PMB24542</skos:changeNote>
  </skos:Concept>
  <skos:Concept rdf:about="http://data.cmbv.org/vocabulary/4d0d7ccb-5cc5-4a51-8c02-54b749211679">
    <skos:inScheme rdf:resource="http://data.cmbv.org/vocabulary/"/>
    <skos:prefLabel xml:lang="fr">Avant-propos</skos:prefLabel>
    <skos:definition xml:lang="fr">Courte introduction (présentation, avis au lecteur, etc.) rédigée par l'auteur d'un ouvrage. (Le Robert)</skos:definition>
    <skos:scopeNote xml:lang="fr"/>
    <skos:changeNote xml:lang="fr"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://data.cmbv.org/vocabulary/6be91254-1bfd-4d4c-b041-663899af8c4b">
    <skos:inScheme rdf:resource="http://data.cmbv.org/vocabulary/"/>
    <skos:prefLabel xml:lang="fr">Ave Maria</skos:prefLabel>
    <skos:definition xml:lang="fr">Prière catholique d’intercession adressée à la Vierge Marie, dont le texte commence par les mots latins Ave Maria (« Je vous salue Marie »).</skos:definition>
    <skos:scopeNote xml:lang="fr">Le texte de l’Ave Maria a été mis en musique par de nombreux compositeurs.</skos:scopeNote>
    <skos:changeNote xml:lang="fr"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://data.cmbv.org/vocabulary/729f5eb5-3a08-4073-a603-38cba5013dad">
    <skos:inScheme rdf:resource="http://data.cmbv.org/vocabulary/"/>
    <skos:prefLabel xml:lang="fr">Avent</skos:prefLabel>
    <skos:definition xml:lang="fr">Temps liturgique chrétien précédant Noël, constitué de quatre semaines marquant la préparation à la célébration de la naissance du Christ. Il ouvre l’année liturgique.</skos:definition>
    <skos:broader rdf:resource="http://data.cmbv.org/vocabulary/67bad974-d2f8-442f-b401-ae0ff462a787"/>
    <skos:scopeNote xml:lang="fr"/>
    <skos:changeNote xml:lang="fr">PMB1163</skos:changeNote>
  </skos:Concept>
  <skos:Concept rdf:about="http://data.cmbv.org/vocabulary/1704e092-82f3-4fb5-add2-607741616902">
    <skos:inScheme rdf:resource="http://data.cmbv.org/vocabulary/"/>
    <skos:prefLabel xml:lang="fr">Avent, dimanche i</skos:prefLabel>
    <skos:definition xml:lang="fr">Premier des quatre dimanches du temps de l’Avent, symboliquement associé au pardon originel d’Adam et Ève.</skos:definition>
    <skos:altLabel xml:lang="fr">1er dimanche de l’Avent</skos:altLabel>
    <skos:narrower rdf:resource="http://data.cmbv.org/vocabulary/67bad974-d2f8-442f-b401-ae0ff462a787"/>
    <skos:scopeNote xml:lang="fr"/>
    <skos:changeNote xml:lang="fr"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://data.cmbv.org/vocabulary/ace93832-1ea8-4c9c-8484-02004686ed7a">
    <skos:inScheme rdf:resource="http://data.cmbv.org/vocabulary/"/>
    <skos:prefLabel xml:lang="fr">Avent, dimanche ii</skos:prefLabel>
    <skos:definition xml:lang="fr">Deuxième dimanche du temps de l’Avent, associé symboliquement à la foi des Patriarches et à l’attente de la Terre promise.</skos:definition>
    <skos:altLabel xml:lang="fr">2e dimanche de l’Avent</skos:altLabel>
    <skos:scopeNote xml:lang="fr"/>
    <skos:changeNote xml:lang="fr"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://data.cmbv.org/vocabulary/494b9982-2a54-413c-b3ab-b70b4698b2ef">
    <skos:inScheme rdf:resource="http://data.cmbv.org/vocabulary/"/>
    <skos:prefLabel xml:lang="fr">Avent, dimanche iii</skos:prefLabel>
    <skos:definition xml:lang="fr">Troisième dimanche de l’Avent, traditionnellement associé à la joie du roi David célébrant l’Alliance.</skos:definition>
    <skos:altLabel xml:lang="fr">3e dimanche de l’Avent</skos:altLabel>
    <skos:scopeNote xml:lang="fr"/>
    <skos:changeNote xml:lang="fr"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://data.cmbv.org/vocabulary/67bad974-d2f8-442f-b401-ae0ff462a787">
    <skos:inScheme rdf:resource="http://data.cmbv.org/vocabulary/"/>
    <skos:prefLabel xml:lang="fr">Avent, dimanche iv</skos:prefLabel>
    <skos:definition xml:lang="fr">Dernier dimanche de l’Avent, symboliquement lié à l’enseignement des Prophètes annonçant un règne de paix et de justice.</skos:definition>
    <skos:altLabel xml:lang="fr">4e dimanche de l’Avent</skos:altLabel>
    <skos:broader rdf:resource="http://data.cmbv.org/vocabulary/1704e092-82f3-4fb5-add2-607741616902"/>
    <skos:narrower rdf:resource="http://data.cmbv.org/vocabulary/729f5eb5-3a08-4073-a603-38cba5013dad"/>
    <skos:scopeNote xml:lang="fr"/>
    <skos:changeNote xml:lang="fr"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://data.cmbv.org/vocabulary/40029258-a5f4-45f4-b92a-f1c9977abcea">
    <skos:inScheme rdf:resource="http://data.cmbv.org/vocabulary/"/>
    <skos:prefLabel xml:lang="fr">Avertissement (préface)</skos:prefLabel>
    <skos:definition xml:lang="fr">Petite préface pour attirer l'attention du lecteur. (Le Robert)</skos:definition>
    <skos:scopeNote xml:lang="fr"/>
    <skos:changeNote xml:lang="fr"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://data.cmbv.org/vocabulary/ed98a31a-3a14-490e-be59-90128be16526">
    <skos:inScheme rdf:resource="http://data.cmbv.org/vocabulary/"/>
    <skos:prefLabel xml:lang="fr">Babiole</skos:prefLabel>
    <skos:definition xml:lang="fr">Chose de peu de valeur et puérile. (Furetière, Antoine (1701). Dictionnaire universel...)</skos:definition>
    <skos:scopeNote xml:lang="fr"/>
    <skos:changeNote xml:lang="fr">PMB1164</skos:changeNote>
  </skos:Concept>
  <skos:Concept rdf:about="http://data.cmbv.org/vocabulary/d6963a61-ea67-499f-b060-15f65462a30f">
    <skos:inScheme rdf:resource="http://data.cmbv.org/vocabulary/"/>
    <skos:prefLabel xml:lang="fr">Bacchanales</skos:prefLabel>
    <skos:definition xml:lang="fr">Fêtes des anciens Grecs et Romains célébrées en l'honneur de Bacchus, et accompagnées de musique. On donnait aussi ce nom à certaines compositions vocales, ordinairement sans instruments, écrites sur des poésies burlesques et populaires, qui ressemblaient aux chants du carnaval, jadis en usage à Florence, surtout au temps des Médicis. — On nomme aussi Bacchanale une certaine danse bruyante et tumultueuse introduite dans un ballet. (Escudier, Léon et Marie (1872). Dictionnaire de musique théorique et historique (5e éd.). E. Dentu.)</skos:definition>
    <skos:scopeNote xml:lang="fr"/>
    <skos:changeNote xml:lang="fr">PMB1165</skos:changeNote>
  </skos:Concept>
  <skos:Concept rdf:about="http://data.cmbv.org/vocabulary/5f1f021f-a88b-4272-80ce-70dcad5cc808">
    <skos:inScheme rdf:resource="http://data.cmbv.org/vocabulary/"/>
    <skos:prefLabel xml:lang="fr">Bal</skos:prefLabel>
    <skos:definition xml:lang="fr">Assemblée de personnes qui se sont réunies pour danser aux sons des instruments. (Escudier, Léon et Marie (1872). Dictionnaire de musique théorique et historique (5e éd.). E. Dentu.)</skos:definition>
    <skos:narrower rdf:resource="http://data.cmbv.org/vocabulary/3a57e5d0-f6c9-4a61-85e9-e47f0296961d"/>
    <skos:narrower rdf:resource="http://data.cmbv.org/vocabulary/3a57e5d0-f6c9-4a61-85e9-e47f0296961d"/>
