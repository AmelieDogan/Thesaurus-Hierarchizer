    <skos:inScheme rdf:resource="http://data.cmbv.org/vocabulary/"/>
    <skos:prefLabel xml:lang="fr">Adaptation d'oeuvre musicale</skos:prefLabel>
    <skos:definition xml:lang="fr">Résultat de la modification d'une ou plusieurs oeuvres préexistantes.</skos:definition>
    <skos:scopeNote xml:lang="fr"/>
    <skos:changeNote xml:lang="fr">EP345704</skos:changeNote>
