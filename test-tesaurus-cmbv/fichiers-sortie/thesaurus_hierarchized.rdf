  <skos:Concept rdf:about="http://data.cmbv.org/vocabulary/2c91a957-e1e1-4873-91aa-cce88c510dee">
    <skos:inScheme rdf:resource="http://data.cmbv.org/vocabulary/"/>
    <skos:prefLabel xml:lang="fr">Assomption</skos:prefLabel>
    <skos:definition xml:lang="fr">Dans la tradition chrétienne, élévation de la Vierge Marie au ciel à la fin de sa vie terrestre. La fête de l’Assomption est célébrée le 15 août.</skos:definition>
    <skos:altLabel xml:lang="fr">Elévation de Marie</skos:altLabel>
    <skos:altLabel xml:lang="fr">Dormition</skos:altLabel>
    <skos:altLabel xml:lang="fr">15 août</skos:altLabel>
    <skos:scopeNote xml:lang="fr"/>
    <skos:changeNote xml:lang="fr">PMB1161</skos:changeNote>
  </skos:Concept>
